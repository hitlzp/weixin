/* pages/login/login.wxss */
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.inputSection {
    border-bottom: 1px solid #9acd32;
    margin-bottom: 20px;
}
.submitBtn {
    height: 30px;
    line-height: 30px;
    border-radius: 6px;
    background-color: #9acd32;
}
.userRegister {
    font-size: 12px;
    float: right;
    color: #9acd32;
    margin-top: 10px;
}