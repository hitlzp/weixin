.container {
  height: 100%;
  padding: 100rpx 0;
  padding-top: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.num-text {
    color: #9acd32;
    font-weight: bold;
}
.navigator {
    font-size: 14px;
    color: #9acd32;
    margin-bottom: 20px;
}
.navigator-hover {
    text-decoration: underline;
}

.table {  
    border: 0px solid darkgray;
}
.tr {  
    display: flex;  
    width: 100%;  
    justify-content: center;  
    height: 2rem;  
    align-items: center;
}

.tr2 {  
    display: flex;  
    width: 100%;  
    justify-content: center;  
    height: 2rem;  
    align-items: center;
    color:#ff0000;
}

.td {    
    width:40%;    
    justify-content: center;    
    text-align: center;
}
.bg-w{  
    background: snow;
}
.bg-g{  
    background: #E6F3F9;
}
.th {  
    width: 49%;  
    justify-content: center;  
    background: #3366FF;  
    color: #fff;  
    display: flex;  
    height: 2rem;  
    align-items: center;
}
.btn-section {
    display: flex;
    justify-content: space-between;
    position:fixed;
    bottom:0px;
    width:100%;
}

.btn-left, .btn-right {
    height: 30px;
    width: 45%;
    line-height: 30px;
    border-radius: 6px;
    background-color: #9acd32;
}